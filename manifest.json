{
    "manifest_version": 2,
    "name": "Turtles 3 App",
    "author": "Voyage 2 Turtles 3",
    "version": "0.0.1",
 "content_security_policy":"script-src 'self' https://api.darksky.net https://use.fontawesome.com https://www.gstatic.com/ https://ipinfo.io/  https://*.firebaseio.com https://www.googleapis.com; object-src 'self'",
  "description": "Turtles Dashboard",
    "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["/css/style.css"],
      "js": ["js/firebase.js","/js/main.js","/js/skycons.js", "js/jquery-3.2.1.js"]
    }
  ],
    "key": "",
  "browser_action": {
		"default_icon": "img/favicon.png"
	},
	
    "background": {
        "scripts": [ "js/background.js" ]
    },

    "oauth2": {
    "client_id": "145357060562-mrcckjd4tvluvoj2pqbomliq085sfu81.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },
	
	"permissions": [
		"tabs",
		"geolocation",
		"activeTab",
		 "identity"
	],
	
	"chrome_url_overrides": {
		"newtab": "/newtab.html"
	}	
}