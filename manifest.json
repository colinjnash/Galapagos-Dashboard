{
    "manifest_version": 2,
    "name": "Turtles 3 App",
    "version": "1.0.8",
    
	"browser_action": {
		"default_icon": "img/favicon.png"
	},
	
    "background": {
        "scripts": [ "js/background.js" ]
    },
	
	"permissions": [
		"tabs"
	],
	
	"chrome_url_overrides": {
		"newtab": "/newtab.html"
	}	
}